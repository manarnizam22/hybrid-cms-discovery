NODE_ENV=development
PORT=3000

# Database Configuration (PostgreSQL - Self-Hosted)
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_NAME=cms_db
DB_SSL=false
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/cms_db

# Redis Configuration (Self-Hosted or ElastiCache)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key

# Amazon S3 Configuration
AWS_S3_BUCKET_NAME=cms-media-bucket

# Amazon SQS Configuration
AWS_SQS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/123456789012/thmanyah-platform-dev-indexing-queue

# Amazon OpenSearch Configuration
OPENSEARCH_ENDPOINT=https://search-cms-opensearch-domain.us-east-1.es.amazonaws.com
OPENSEARCH_INDEX_NAME=cms_content

# Security Configuration
ALLOWED_ORIGINS=http://localhost:3000,https://yourdomain.com
API_KEY=your-optional-api-key-for-cms-endpoints

# Application Configuration
LOG_LEVEL=info